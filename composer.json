{
    "name": "enomotodev/circleci-composer-update-pr",
    "description": "Create PullRequest of composer update in CircleCI",
    "keywords": ["composer", "circleci", "github"],
    "license": "MIT",
    "type": "library",
    "homepage": "https://enomotodev.hatenablog.com/",
    "support": {
        "issues": "https://github.com/enomotodev/circleci-composer-update-pr/issues",
        "source": "https://github.com/enomotodev/circleci-composer-update-pr"
    },
    "authors": [
        {
            "name": "enomotodev",
            "email": "enomoto.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "davidrjonas/composer-lock-diff": "^1.0@dev",
        "knplabs/github-api": "^2.9",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "bin": [
        "circleci-composer-update-pr"
    ],
    "autoload": {
        "psr-4": {
            "Enomotodev\\CircleCIComposerUpdatePr\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
